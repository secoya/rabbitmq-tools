{
	"name": "@secoya/rabbitmq-tools",
	"description": "Tools for interfacing with RabbitMQ",
	"repository": "https://github.com/secoya/rabbitmq-tools",
	"author": "Kaare Hoff Skovgaard <khs@secoya.dk>",
	"license": "MIT",
	"private": true,
	"type": "module",
	"prettier": "@secoya/prettier-config",
	"scripts": {
		"build": "tools/build.sh"
	},
	"main": "./index.cjs",
	"types": "./index.d.ts",
	"exports": {
		".": {
			"import": "./index.js",
			"require": "./index.cjs",
			"types": "./index.d.ts"
		},
		"./context.js": {
			"import": "./context.js",
			"require": "./context.cjs",
			"types": "./context.d.ts"
		}
	},
	"devDependencies": {
		"@orbit-online/git-release": "^0.2.0",
		"@rushstack/eslint-patch": "1.2.0",
		"@secoya/context-helpers": "3.4.11",
		"@secoya/eslint-plugin-orbit": "1.1.3",
		"@secoya/nodejs-build-tools": "4.0.1",
		"@secoya/prettier-config": "1.0.0",
		"@types/amqplib": "0.8.2",
		"eslint": "8.25.0",
		"opentracing": "0.14.7",
		"prettier": "2.7.1",
		"tslib": "2.4.1",
		"tsup": "6.5.0",
		"typescript": "4.8.4"
	},
	"dependencies": {
		"@types/amqplib": "0.8.2",
		"amqplib": "0.8.0",
		"rxjs": "6.5.5"
	},
	"peerDependencies": {
		"@secoya/context-helpers": "3.2.0",
		"opentracing": "^0.14.7",
		"tslib": "^2.4.0"
	},
	"peerDependenciesMeta": {
		"@secoya/context-helpers": {
			"optional": true
		},
		"opentracing": {
			"optional": true
		}
	}
}
